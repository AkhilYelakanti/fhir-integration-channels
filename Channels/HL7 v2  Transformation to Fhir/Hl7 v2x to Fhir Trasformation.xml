        <script>	
	var messageData=connectorMessage.getRawData();
	var serverbase=&quot;http://192.168.225.194:2019&quot;;
//	var adsLength = messageData.length;
//	var base64Bytes = [];
//	for(i = 0; i &lt; adsLength;i++){
//	    base64Bytes.push(messageData.charCodeAt(i));
//	}
//	var encodedData = FileUtil.encode(base64Bytes);
	var encodedData =FileUtil.encode(new java.lang.String(messageData).getBytes(&apos;UTF-8&apos;));
	encodedData=encodedData.split(&quot;\r\n&quot;).join(&quot;&quot;);
	logger.info(&quot;HL7 message encoded data &quot;+encodedData);
	var template_id=$(&apos;template_id&apos;)+&quot;.hbs&quot;;
	//var baseurl= $(&apos;fhir_base_url&apos;);
	var baseurl = sourceMap.get(&apos;fhir_base_url&apos;);
	var token = &quot;Bearer &quot;+ sourceMap.get(&apos;token&apos;);
	var url = new java.net.URL(serverbase+&quot;/api/templates/&quot;+template_id);
    // Open connection to url
     var conn = url.openConnection();

    // Send request   
     conn.setRequestMethod(&quot;GET&quot;);
     conn.setRequestProperty(&quot;Content-Language&quot;, &quot;en-US&quot;);
     conn.setDoOutput(true);


     //Capture Response
     logger.info(conn.getResponseCode());
     var respCode = conn.getResponseCode();
     var inputStream = conn.getInputStream();
     var streamReader = new java.io.InputStreamReader(inputStream);
     var respStream = new java.io.BufferedReader(streamReader);
     var buffer = &apos;&apos;;
     var line = null;
     while ((line = respStream.readLine()) != null) {
         buffer = buffer + line;
     }
     respStream.close();
     
 	//do something with results
 	
 	var templateEncodedData =FileUtil.encode(new java.lang.String(buffer).getBytes(&apos;UTF-8&apos;));
 	templateEncodedData=templateEncodedData.split(&quot;\r\n&quot;).join(&quot;&quot;);
 	
 	// open new url connection to convert 

 	var convertUrl = new java.net.URL(serverbase+&quot;/api/convert/hl7&quot;);
 	 var newconn = convertUrl.openConnection();
 	  // Send request   
     newconn.setRequestMethod(&quot;POST&quot;);
     newconn.setRequestProperty(&quot;Content-Type&quot;, &quot;application/json&quot;);
     newconn.setRequestProperty(&quot;Content-Language&quot;, &quot;en-US&quot;);
     newconn.setUseCaches(false);
     newconn.setDoInput(true);
     newconn.setDoOutput(true);

     var reqBody=&quot;{\&quot;templateBase64\&quot;:\&quot;&quot;+templateEncodedData+&quot;\&quot;,\&quot;messageBase64\&quot;:\&quot;&quot;+encodedData+&quot;\&quot;} &quot;;
	
      
//      reqBody.add(&quot;templateBase64&quot;,templateEncodedData);
//      reqBody.add(&quot;messageBase64&quot;,encodedData);
     var convertOutStream = newconn.getOutputStream();
     var convOutWriter = new java.io.OutputStreamWriter(convertOutStream);
     //create entity
     convOutWriter.write(reqBody.toString());
     convOutWriter.close();

     //Capture Response
     logger.info(newconn.getResponseCode());
     var respCodeConvert = newconn.getResponseCode();
     var inputStreamConvert = newconn.getInputStream();
     var streamReaderConvert = new java.io.InputStreamReader(inputStreamConvert);
     var respStreamConvert = new java.io.BufferedReader(streamReaderConvert);
     var bufferConvert = &apos;&apos;;
     var line1 = null;
     while ((line1 = respStreamConvert.readLine()) != null) {
         bufferConvert = bufferConvert + line1;
     }
     respStreamConvert.close();
 	logger.info(&quot;Converted json &quot;+bufferConvert);
     return bufferConvert;  
   
   </script>
